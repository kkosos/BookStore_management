<!DOCTYPE html>
<html>
<head>
	<link rel="stylesheet" type="text/css" href="semUI_out/semantic.min.css">
	<link rel = "stylesheet" type = "text/css" href = "/stylesheets/index.css"/> 
	<script src="/jquery/jquery.min.js"></script>
	<script src="semUI_out/semantic.min.js"></script>
    
	<script src="/javascripts/main_page.js"></script> 
	<script src="/socket.io/socket.io.js"></script> 
	<script src="/javascripts/notify.js"></script> 
</head>
<body>
<% var data=id ;%>

<% include side_top_bar%>

<div id="main" class="ui grid">
	<div class="row">		
		<div class="three wide column"></div>
		<div class="six wide column">
				<div class="ui message">
					<i class="close icon"></i>
						<div class="header">
							Welcome back
						</div>
					<p>Hello,<%= id %>
					</p>
				</div>
		</div>
		<div class="six wide column"></div>
		<div class="two wide column"></div>
	</div>
	<div class="row">
			<div class="three wide column"></div>
			<div class="six wide column">
					<% if(results!="") {%>
					<div class="ui red warnnig message">
							<div class="header">
							Some books are going to out of stock 
						</div>
						<a href="/users/get_stock_list?toggle=true">Check here!</a>
					</div>
					<% } %>
			
		
			</div>
			<div class="six wide column">		
				<div class="ui yellow message">
							<div class="header">
							A message coming
						</div>
						<a href="">Check here!</a>
				</div>
						
			</div>
			<div class="three wide column"></div>
	</div>
	<div class="row">
				<div class="three wide column"></div>
				<div class="ten wide column">
					<table class="ui celled striped table">
							<thead>
								<tr>
									<th colspan="3">
									Transaction list
									</th>
								</tr>
								<tr>
									<th>Transaction_id</th>
									<th>name</th>
									<th>date</th>
								</tr>
							</thead>

							<tbody>
								<% for(var i in trade_result){ %>
								<tr>
									<td><%= trade_result[i].trade_id %></td>
									<td><%= trade_result[i].name %></td>
									<td><%= trade_result[i].date %></td>
								</tr>
								<% } %>
								
							</tbody>
						</table>
				</div>
				<div class="three wide column"></div>	
	</div>		
				
				
				
</div>


<script>
	var warn_list=[];
	warn_list= <%- JSON.stringify(results) %> ;
	$('.message .close').on('click',function(){
							$(this).closest('.message').transition('fade')
							
							});

</script>
     <script src="/javascripts/warning_list.js"></script> 
</body>
</html> 

